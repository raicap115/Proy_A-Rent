@{
    ViewData["Title"] = "Disponibilidad";
}
@model Bookings;
<div class="container">
    <h1 class="display-4" style="font-weight: bold; margin-top: 30px; text-align: center;">Disponibilidad del Auto</h1>
</div>


<form action="" method="post">

    <div asp-validation-summary="All"></div>

    <div class="form-group">
        <label>Correo:</label>
        <input name="correo" class="form-control" required />
    </div>  
    <div class="container">
        <label asp-for="respuesta"></label>
        <strong style="color: #157D0B;">@Html.DisplayTextFor(model => model.respuesta)</strong>
    </div>

    <div style="text-align: center;">
        <button type="submit" class="btn btn-dark">Rentar</button>
    </div>
</form>