<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - A-RENT</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/sit.css" />
</head>
<body>
    <div class="container">
        <div style="text-align: center;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        </div>
    </div>

    <footer class="border-top footer text-muted" style="background-color: #eb5757; text-align: center; opacity: 40%;">
        <div class="container" style="color: #161515;">
            &copy; A-RENT - Todos los derechos reservados
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
